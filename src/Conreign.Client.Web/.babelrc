{
  "presets": [
    ["env", {
      "loose": true,
      "targets": {
        "browsers": "> 1%"
      },
      "modules": false,
      "useBuiltIns": true
    }],
    "react",
    "stage-2"
  ],
  "sourceMaps": true,
  "plugins": [
    "lodash"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
